{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "skipLibCheck": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "types": ["@emotion/react"],
    "noEmit": true,
  },
  "include": ["../site/src/@types/**.ts", "**/*.tsx"]
}
