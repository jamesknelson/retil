{
  "name": "retil",
  "version": "0.1.3",
  "description": "A collection of simple hooks and functions for use with React",
  "author": "James K Nelson <james@jamesknelson.com>",
  "license": "MIT",
  "main": "dist/commonjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build:commonjs": "tsc --pretty --module commonjs --outDir dist/commonjs",
    "build:es": "tsc --pretty --module es2015 --outDir dist/es",
    "build:types": "tsc --pretty --declaration --emitDeclarationOnly --outDir dist/types --isolatedModules false",
    "build": "yarn run clean && yarn build:es && yarn build:commonjs && yarn build:types",
    "build:watch": "yarn run clean && yarn build:es -- --types --watch",
    "lint": "eslint --ext ts,tsx src",
    "prepare": "yarn test && yarn build",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@types/history": "^4.7.5",
    "abort-controller": "^3.0.0",
    "redux": "^4.0.5",
    "tslib": "1.10.0",
    "use-subscription": "^1.3.0"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^3.2.1",
    "@types/jest": "^24.0.19",
    "@types/node": "^12.11.7",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "@types/use-subscription": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.0.2",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "jest": "^24.9.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-test-renderer": "^16.12.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.3.0",
    "typescript": "^3.7.4"
  },
  "peerDependencies": {
    "react": "^16.12.0"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "resource",
    "cache",
    "suspense"
  ]
}
