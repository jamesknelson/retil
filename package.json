{
  "name": "retil",
  "version": "0.0.1",
  "description": "A collection of simple hooks and functions for use with React",
  "author": "James K Nelson <james@jamesknelson.com>",
  "license": "MIT",
  "main": "dist/commonjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build:commonjs": "tsc --pretty --module commonjs --outDir dist/commonjs",
    "build:es": "tsc --pretty --module es2015 --outDir dist/es",
    "build:types": "tsc --pretty --declaration --emitDeclarationOnly --outDir dist/types --isolatedModules false",
    "build": "yarn run clean && yarn build:es && yarn build:commonjs && yarn build:types",
    "build:watch": "yarn run clean && yarn build:es -- --types --watch",
    "prepare": "yarn test && yarn build",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "abort-controller": "^3.0.0",
    "memoize-one": "^5.1.1",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@types/node": "^12.11.7",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "cross-env": "^6.0.3",
    "eslint-config-react-app": "^5.0.2",
    "jest": "^24.9.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-test-renderer": "^16.12.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.25.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "ts-jest": "^24.1.0",
    "typescript": "^3.7.4"
  },
  "peerDependencies": {
    "react": "^16.12.0"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "resource",
    "cache",
    "suspense"
  ],
  "jest": {
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx"
    ],
    "modulePaths": ["<rootDir>/src/"],
    "preset": "ts-jest",
    "testMatch": null,
    "globals": {
      "ts-jest": {
        "babelConfig": null,
        "diagnostics": false
      }
    }
  }
}
