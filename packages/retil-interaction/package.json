{
  "name": "retil-interaction",
  "version": "0.23.0",
  "description": "Utilities to create unstyled interactive components",
  "author": "James K Nelson <james@jamesknelson.com>",
  "license": "MIT",
  "main": "dist/commonjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/es/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build:commonjs": "tsc -p tsconfig.build.json --module commonjs --outDir dist/commonjs",
    "build:es-and-types": "tsc -p tsconfig.build.json --module es2015 --outDir dist/es --declaration",
    "build": "yarn run clean && yarn build:es-and-types && yarn build:commonjs",
    "build:watch": "yarn run clean && yarn build:es-and-types -- --watch",
    "lint": "eslint --ext ts,tsx src",
    "prepare": "yarn test && yarn build",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@popperjs/core": "^2.9.1",
    "fast-deep-equal": "^3.1.3",
    "focus-trap": "^6.6.1",
    "retil-css": "^0.23.0",
    "retil-source": "^0.23.0",
    "retil-support": "^0.23.0",
    "tslib": "^2.2.0"
  },
  "devDependencies": {
    "@types/styled-components": "^5.1.7",
    "jest-styled-components": "^7.0.3",
    "styled-components": "^5.3.0",
    "typescript": "4.3.5"
  },
  "peerDependencies": {
    "react": "^17.0.1"
  },
  "files": [
    "dist"
  ],
  "gitHead": "1fa948a087d38a7f3747425f593098f0614dc180"
}
