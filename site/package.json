{
  "name": "retil.tech",
  "licence": "CC-BY-NC-4.0",
  "private": true,
  "version": "0.20.1",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "dev": "node server",
    "dev:spa": "vite",
    "build:client": "yarn typecheck && vite build --outDir dist/client && mv dist/client/index.html dist/server",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "build": "yarn build:server && yarn build:client",
    "generate": "vite build --outDir dist/static && yarn build:server && node prerender",
    "serve": "cross-env NODE_ENV=production node server",
    "debug": "node --inspect-brk server"
  },
  "dependencies": {
    "@emotion/babel-plugin": "^11.3.0",
    "@emotion/css": "^11.1.3",
    "@emotion/react": "^11.4.0",
    "@emotion/server": "^11.4.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "polished": "^4.1.3",
    "react-helmet-async": "^1.0.9",
    "retil-boundary": "^0.20.1",
    "retil-hydration": "^0.20.1",
    "retil-interaction": "^0.20.1",
    "retil-mount": "^0.20.1",
    "retil-nav": "^0.20.1",
    "retil-support": "^0.20.1",
    "slugify": "^1.5.1",
    "tslib": "^2.2.0"
  },
  "devDependencies": {
    "@mdx-js/mdx": "^2.0.0-next.9",
    "@mdx-js/react": "^1.6.22",
    "@types/express": "^4.17.11",
    "@types/mdx-js__react": "^1.5.3",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "serve-static": "^1.14.1",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "3.9.0",
    "vite": "^2.3.4",
    "vite-plugin-mdx": "^3.5.6",
    "vite-tsconfig-paths": "3.3.11"
  }
}
