{
  "compilerOptions": {
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react",
    "baseUrl": "src",
    "importHelpers": true,
    "removeComments": false,
    "resolveJsonModule": true,
    "sourceMap": false,
    "strict": true,
    "strictPropertyInitialization": false,
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "es2015",
      "es2016",
      "es2017"
    ]
  },
}
