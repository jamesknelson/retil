{
  "compilerOptions": {
    "allowJs": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react",
    "baseUrl": "src",
    "importHelpers": true,
    "removeComments": false,
    "resolveJsonModule": true,
    "sourceMap": false,
    "strict": true,
    "strictPropertyInitialization": false,
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ]
  },
}
